  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ushnuel/Teamwork/issues"
  },
  "homepage": "https://github.com/ushnuel/Teamwork#readme",
  "dependencies": {
    "@babel/core": "^7.6.4",
    "@babel/polyfill": "^7.6.0",
    "bcrypt": "^5.0.0",
    "braces": "^2.3.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.16.0",
    "cors": "^2.8.5",
    "datauri": "^2.0.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
